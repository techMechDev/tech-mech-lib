{
  "name": "tech-mech-lib",
  "version": "0.0.4",
  "description": "Components library",
  "main": "./dist",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "new:component": "node ./scripts/create-component.js",
    "test": "jest --silent=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:clean": "node ./scripts/clean.js",
    "build:tokens": "node ./scripts/buildTokens",
    "build:copy": "node ./scripts/copy.js",
    "build:lib": "npx babel ./src --out-dir ./lib && npm run build:tokens && npm run build:copy",
    "build:dist": "webpack",
    "build:pro": "npm run build:clean && npm run build:lib && npm run build:dist"
  },
  "keywords": [
    "componentes",
    "components",
    "library",
    "react",
    "npm"
  ],
  "author": {
    "name": "techMechDev",
    "email": "techmech.dev81@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.3",
    "@babel/plugin-transform-class-properties": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/react-webpack5": "7.5.3",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "minireset.css": "0.0.7",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.5.3",
    "style-loader": "3.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "peerDependencies": {
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
